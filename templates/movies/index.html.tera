{% extends "layouts/base" %}

{% block content %}	
	<h1 class="py-4">Movies</h1>
		<table class="table table-hover table-bordered">
			<thead>
				<tr>
					<th> Name </th>
					<th> Director </th>
				</tr>
			</thead>
			<tbody>
				{% for movie in movies%}
					<tr>
						<td> {{ movie.name }} </td>
						<td> {{ movie.director }} </td>
					</tr>
				{% endfor %}
			</tbody>
		</table>
	</div>
{% endblock %}
